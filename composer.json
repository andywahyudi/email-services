{
    "name": "andywahyudi/multi-email-services",
    "description": "A PHP library for handling multiple email service providers with failover support",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "mailgun/mailgun-php": "^3.5",
        "sendgrid/sendgrid": "^8.0",
        "wildbit/postmark-php": "^4.0",
        "symfony/http-client": "^5.4|^6.0",
        "nyholm/psr7": "^1.8",
        "phpmailer/phpmailer": "^6.9"
    },
    "autoload": {
        "psr-4": {
            "MultiEmailServices\\Library\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
